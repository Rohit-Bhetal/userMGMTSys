<div class="d-flex justify-content-between flex-wrap flex-wrap flex-md nowrap align-item-center pt-3 pb-2 mb-3 border-bottom">
    <h1 class="h2">Search</h1>
    <div class="btn-toolbar mb-2 mb-md 0">
        <div class="btn-group me-2 pe-1">
            <a href="/add" class="btn btn-sm btn-outline-secondary">+ New User</a>
        </div>
    </div>
</div>

<div class="rable-responsive">
    <% if (customers>0) { %>
        <table class="table table-striped table-sm">
            <thead>
                <tr>
                    <th scope="col">First Name</th>
                    <th scope="col">Last Name</th>
                    <th scope="col">Telephone</th>
                    <th scope="col">Email</th>
                </tr>
            </thead>
            <tbody>
                <% customers.forEach(element => { %>
                 <td><%= element.firstName %></td>
                 <td><%= element.lastName %></td>
                 <td><%= element.tel %></td>
                 <td><%= element.lastName %></td>
                <% }) %>
            </tbody>
        </table>
    <% }else{ %>
        <h2>No Results Found.</h2>
        <% }
</div>